{
  "name": "static-site-seed",
  "description": "Static website seed",
  "version": "1.0.0",
  "author": "James Adarich",
  "dependencies": {
    "express": "^4.16.2",
    "gatsby": "^1.9.199",
    "gatsby-cli": "^1.1.40",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-canonical-urls": "^1.0.13",
    "gatsby-plugin-google-tagmanager": "^1.0.14",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-plugin-sass": "^1.0.17",
    "gatsby-plugin-sitemap": "^1.2.13",
    "gatsby-plugin-typescript": "^1.4.16",
    "gatsby-source-filesystem": "^1.5.20",
    "gatsby-transformer-remark": "^1.7.32",
    "glob": "^7.1.2",
    "greenlock-express": "^2.0.12",
    "le-challenge-fs": "^2.0.8",
    "le-store-certbot": "^2.0.5",
    "react-helmet": "^5.2.0",
    "redirect-https": "^1.1.4",
    "typescript": "^2.7.2"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:server && npm run build:gatsby",
    "build:http": "npm run build:gatsby && tsc server/http-startup.ts --module commonjs --target esnext",
    "build:server": "tsc server/startup.ts --module commonjs --target esnext",
    "build:gatsby": "tsc gatsby-browser.ts gatsby-config.ts gatsby-node.ts gatsby-ssr.ts --module commonjs --target esnext && gatsby build",
    "develop": "npm run build && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "lint": "npm run lint:sass && npm run lint:typescript",
    "lint:fix": "npm run lint:sass:fix && npm run lint:typescript:fix",
    "lint:sass": "sass-lint \"**/*.scss\" --ignore \"node_modules/**/*.*\"",
    "lint:sass:fix": "sass-lint \"**/*.scss\" --ignore \"node_modules/**/*.*\" --fix ",
    "lint:typescript": "tslint \"**/*.ts\" \"**/*.tsx\" --exclude \"node_modules/**/*.*\"",
    "lint:typescript:fix": "tslint --fix \"**/*.ts\" \"**/*.tsx\" --exclude \"node_modules/**/*.*\"",
    "prestart": "npm run build",
    "start": "node ./server/startup.js",
    "start:http": "npm run build:http && node ./server/http-startup.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/prop-types": "^15.5.2",
    "@types/react-helmet": "^5.0.4",
    "prettier": "^1.10.2",
    "sass-lint": "^1.12.1",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^1.3.0"
  }
}
