{
  "name": "static-site-seed",
  "description": "Static website seed",
  "version": "1.0.0",
  "author": "James Adarich",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.12",
    "@types/prop-types": "^15.5.2",
    "@types/react-helmet": "^5.0.4",
    "core-js": "^2.5.7",
    "eslint": "^5.10.0",
    "gatsby": "^2.0.48",
    "gatsby-cli": "^2.4.17",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-canonical-urls": "^2.0.12",
    "gatsby-plugin-google-tagmanager": "^2.0.10",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sass": "^2.0.4",
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-typescript": "^2.0.1",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.12",
    "gatsby-transformer-sharp": "^2.1.8",
    "glob": "^7.1.3",
    "node-sass": "^4.9.0",
    "prettier": "^1.10.2",
    "prismjs": "^1.15.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "sass-lint": "^1.12.1",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "webpack": "^4.12.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:server && npm run build:gatsby",
    "build:server": "tsc server/startup.ts --module commonjs --target esnext",
    "build:gatsby": "npm run build:gatsby:config && gatsby build",
    "build:gatsby:config": "tsc gatsby-browser.ts gatsby-config.ts gatsby-node.ts gatsby-ssr.ts --module commonjs --target esnext",
    "build:gzip": "tsc server/gzip.ts --module commonjs && node server/gzip",
    "develop": "npm run build:gatsby:config && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "lint": "npm run lint:sass && npm run lint:typescript",
    "lint:fix": "npm run lint:sass:fix && npm run lint:typescript:fix",
    "lint:sass": "sass-lint \"**/*.scss\" --ignore \"node_modules/**/*.*\"",
    "lint:sass:fix": "sass-lint \"**/*.scss\" --ignore \"node_modules/**/*.*\" --fix ",
    "lint:typescript": "tslint \"**/*.ts\" \"**/*.tsx\" --exclude \"node_modules/**/*.*\"",
    "lint:typescript:fix": "tslint --fix \"**/*.ts\" \"**/*.tsx\" --exclude \"node_modules/**/*.*\"",
    "start": "node ./server/startup.js",
    "test": "npm run lint"
  }
}
